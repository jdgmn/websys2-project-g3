<h1>Admin Dashboard</h1>
<p>Welcome, <%= currentUser.firstName %> (<%= currentUser.role %>)</p>
<table>
  <thead>
    <tr>
      <th>User ID</th>
      <th>Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>Status</th>
      <th>Created</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td><%= user.userId %></td>
      <td><%= user.firstName %> <%= user.lastName %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      <td><%= user.accountStatus %></td>
      <td><%= user.createdAt.toLocaleDateString() %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
<p><a href="/users/dashboard">Back to Dashboard</a></p>
